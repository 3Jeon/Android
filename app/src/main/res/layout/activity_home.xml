<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/home_address_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="길음동 "
        android:textColor="@color/black"
        android:textStyle="bold"
        android:textSize="20sp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="30dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>

    <ImageView
        android:id="@+id/home_address_iv"
        android:layout_width="8dp"
        android:layout_height="8dp"
        android:src="@drawable/down_more"
        android:layout_marginStart="1dp"
        app:layout_constraintTop_toTopOf="@id/home_address_tv"
        app:layout_constraintBottom_toBottomOf="@id/home_address_tv"
        app:layout_constraintStart_toEndOf="@id/home_address_tv"/>

    <TextView
        android:id="@+id/home_logout_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" 로그아웃"
        android:textColor="@color/black"
        android:layout_marginEnd="20dp"
        app:layout_constraintTop_toTopOf="@id/home_address_tv"
        app:layout_constraintBottom_toBottomOf="@id/home_address_tv"
        app:layout_constraintEnd_toEndOf="parent"/>

    <TextView
        android:id="@+id/home_myinformation_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="내정보 |"
        android:textColor="@color/black"
        app:layout_constraintTop_toTopOf="@id/home_address_tv"
        app:layout_constraintBottom_toBottomOf="@id/home_address_tv"
        app:layout_constraintEnd_toStartOf="@id/home_logout_tv"/>

    <TextView
        android:id="@+id/wlrmaajrrh_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="지금 먹고싶은 음식종류는?"
        android:textSize="25dp"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@id/home_address_tv"
        android:layout_marginTop="50dp"
        app:layout_constraintTop_toBottomOf="@id/home_address_tv"/>

    <TextView
        android:id="@+id/gkskaks_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="하나만 선택하세요!"
        android:textSize="12sp"
        android:layout_marginTop="25dp"
        app:layout_constraintStart_toStartOf="@id/wlrmaajrrh_tv"
        app:layout_constraintTop_toBottomOf="@id/wlrmaajrrh_tv"/>

<!--@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@-->

    <TextView
        android:id="@+id/home_box_1_tv"
        android:layout_width="100dp"
        android:layout_height="70dp"
        android:background="@color/unclick"
        android:layout_marginTop="25dp"
        android:text="한식"
        android:gravity="center"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="@id/gkskaks_tv"
        app:layout_constraintTop_toBottomOf="@id/gkskaks_tv"/>

    <TextView
        android:id="@+id/home_box_2_tv"
        android:layout_width="100dp"
        android:layout_height="70dp"
        android:background="@color/unclick"
        android:text="일식"
        android:gravity="center"
        android:textColor="@color/black"
        app:layout_constraintTop_toTopOf="@id/home_box_1_tv"
        app:layout_constraintBottom_toBottomOf="@id/home_box_1_tv"
        app:layout_constraintStart_toEndOf="@id/home_box_1_tv"
        app:layout_constraintEnd_toStartOf="@id/home_box_3_tv"/>


    <TextView
        android:id="@+id/home_box_3_tv"
        android:layout_width="100dp"
        android:layout_height="70dp"
        android:background="@color/unclick"
        android:text="중식"
        android:gravity="center"
        android:textColor="@color/black"
        android:layout_marginEnd="25dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/home_box_1_tv"
        app:layout_constraintBottom_toBottomOf="@id/home_box_1_tv" />

    <TextView
        android:id="@+id/home_box_4_tv"
        android:layout_width="100dp"
        android:layout_height="70dp"
        android:background="@color/unclick"
        android:layout_marginTop="20dp"
        android:text="양식"
        android:gravity="center"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="@id/gkskaks_tv"
        app:layout_constraintTop_toBottomOf="@id/home_box_1_tv"/>

    <TextView
        android:id="@+id/home_box_5_tv"
        android:layout_width="100dp"
        android:layout_height="70dp"
        android:background="@color/unclick"
        android:text="치킨"
        android:gravity="center"
        android:textColor="@color/black"
        app:layout_constraintTop_toTopOf="@id/home_box_4_tv"
        app:layout_constraintBottom_toBottomOf="@id/home_box_4_tv"
        app:layout_constraintStart_toEndOf="@id/home_box_4_tv"
        app:layout_constraintEnd_toStartOf="@id/home_box_6_tv"/>


    <TextView
        android:id="@+id/home_box_6_tv"
        android:layout_width="100dp"
        android:layout_height="70dp"
        android:background="@color/unclick"
        android:text="분식"
        android:gravity="center"
        android:textColor="@color/black"
        android:layout_marginEnd="25dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/home_box_4_tv"
        app:layout_constraintBottom_toBottomOf="@id/home_box_4_tv" />

    <TextView
        android:id="@+id/home_box_7_tv"
        android:layout_width="100dp"
        android:layout_height="70dp"
        android:background="@color/unclick"
        android:layout_marginTop="20dp"
        android:text="고기/구이"
        android:gravity="center"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="@id/gkskaks_tv"
        app:layout_constraintTop_toBottomOf="@id/home_box_4_tv"/>

    <TextView
        android:id="@+id/home_box_8_tv"
        android:layout_width="100dp"
        android:layout_height="70dp"
        android:background="@color/unclick"
        android:text="도시락"
        android:gravity="center"
        android:textColor="@color/black"
        app:layout_constraintTop_toTopOf="@id/home_box_7_tv"
        app:layout_constraintBottom_toBottomOf="@id/home_box_7_tv"
        app:layout_constraintStart_toEndOf="@id/home_box_7_tv"
        app:layout_constraintEnd_toStartOf="@id/home_box_9_tv"/>


    <TextView
        android:id="@+id/home_box_9_tv"
        android:layout_width="100dp"
        android:layout_height="70dp"
        android:background="@color/unclick"
        android:text="야식"
        android:gravity="center"
        android:textColor="@color/black"
        android:layout_marginEnd="25dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/home_box_7_tv"
        app:layout_constraintBottom_toBottomOf="@id/home_box_7_tv" />

    <TextView
        android:id="@+id/home_box_10_tv"
        android:layout_width="100dp"
        android:layout_height="70dp"
        android:background="@color/unclick"
        android:layout_marginTop="20dp"
        android:text="패스트푸드"
        android:gravity="center"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="@id/gkskaks_tv"
        app:layout_constraintTop_toBottomOf="@id/home_box_7_tv"/>

    <TextView
        android:id="@+id/home_box_11_tv"
        android:layout_width="100dp"
        android:layout_height="70dp"
        android:background="@color/unclick"
        android:text="디저트"
        android:gravity="center"
        android:textColor="@color/black"
        app:layout_constraintTop_toTopOf="@id/home_box_10_tv"
        app:layout_constraintBottom_toBottomOf="@id/home_box_10_tv"
        app:layout_constraintStart_toEndOf="@id/home_box_10_tv"
        app:layout_constraintEnd_toStartOf="@id/home_box_12_tv"/>


    <TextView
        android:id="@+id/home_box_12_tv"
        android:layout_width="100dp"
        android:layout_height="70dp"
        android:background="@color/unclick"
        android:text="아시안푸드"
        android:gravity="center"
        android:textColor="@color/black"
        android:layout_marginEnd="25dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/home_box_10_tv"
        app:layout_constraintBottom_toBottomOf="@id/home_box_10_tv" />


<!--대충 위치만 잡아두겟서 검은색 버튼을 분명히 만들었을테니까-->
    <Button
        android:id="@+id/button6"
        android:layout_width="300dp"
        android:layout_height="48dp"
        android:layout_marginBottom="33dp"
        android:background="@drawable/round_button_choice"
        android:text="가게 리스트 불러오기"
        android:textColor="@color/white"
        android:textSize="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent" />



</androidx.constraintlayout.widget.ConstraintLayout>