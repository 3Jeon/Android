<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="240dp"
        android:layout_height="207dp"
        android:layout_marginTop="142dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/main_nyam" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="132dp"
        android:layout_height="21dp"
        android:layout_marginTop="23dp"
        app:layout_constraintEnd_toEndOf="@+id/imageView12"
        app:layout_constraintStart_toStartOf="@+id/imageView12"
        app:layout_constraintTop_toBottomOf="@+id/imageView12"
        app:srcCompat="@drawable/word_compare" />

    <Button
        android:id="@+id/kakao_login_btn"
        android:layout_width="343dp"
        android:layout_height="44dp"
        android:background="@drawable/kakao_login_btn"
        android:bottomLeftRadius="12dp"
        android:text="카카오로 로그인"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/google_login_btn"
        app:layout_constraintStart_toStartOf="@+id/google_login_btn"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="496dp" />

    <Button
        android:id="@+id/apple_login_btn"
        android:layout_width="343dp"
        android:layout_height="44dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/apple_login_btn"
        android:bottomLeftRadius="12dp"
        android:text="Apple로 로그인"
        android:textSize="16sp"
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="@+id/google_login_btn"
        app:layout_constraintStart_toStartOf="@+id/google_login_btn"
        app:layout_constraintTop_toBottomOf="@+id/kakao_login_btn"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/google_login_btn"
        android:layout_width="343dp"
        android:layout_height="44dp"
        android:layout_marginTop="12dp"
        android:layout_marginBottom="70dp"
        android:background="@drawable/google_login_btn"
        android:bottomLeftRadius="12dp"
        android:text="Google로 로그인"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/apple_login_btn"
        tools:ignore="MissingConstraints" />




</androidx.constraintlayout.widget.ConstraintLayout>