<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="240dp"
        android:layout_height="207dp"
        android:layout_marginTop="142dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/main_nyam" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="132dp"
        android:layout_height="21dp"
        android:layout_marginTop="23dp"
        app:layout_constraintEnd_toEndOf="@+id/imageView12"
        app:layout_constraintStart_toStartOf="@+id/imageView12"
        app:layout_constraintTop_toBottomOf="@+id/imageView12"
        app:srcCompat="@drawable/word_compare" />

    <Button
        android:id="@+id/kakao_login_btn"
        android:layout_width="343dp"
        android:layout_height="44dp"
        android:layout_marginTop="140dp"
        android:background="@drawable/kakao_login_btn"
        android:bottomLeftRadius="12dp"
        android:text="카카오로 로그인"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView7"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/apple_login_btn"
        android:layout_width="343dp"
        android:layout_height="44dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/apple_login_btn"
        android:bottomLeftRadius="12dp"
        android:text="Apple로 로그인"
        android:textAllCaps="false"
        android:textColor="@color/white"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/kakao_login_btn"
        app:layout_constraintStart_toStartOf="@+id/kakao_login_btn"
        app:layout_constraintTop_toBottomOf="@+id/kakao_login_btn"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/google_login_btn"
        android:layout_width="343dp"
        android:layout_height="44dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/google_login_btn"
        android:bottomLeftRadius="12dp"
        android:text="Google로 로그인"
        android:textAllCaps="false"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/kakao_login_btn"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/kakao_login_btn"
        app:layout_constraintTop_toBottomOf="@+id/apple_login_btn"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/kakao_login_btn"
        tools:layout_editor_absoluteX="86dp"
        tools:layout_editor_absoluteY="445dp" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_marginStart="20dp"
        android:translationZ="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/apple_login_btn"
        app:layout_constraintStart_toStartOf="@+id/apple_login_btn"
        app:layout_constraintTop_toTopOf="@+id/apple_login_btn"
        app:srcCompat="@drawable/apple_symbol" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_marginStart="20dp"
        android:translationZ="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/google_login_btn"
        app:layout_constraintStart_toStartOf="@+id/google_login_btn"
        app:layout_constraintTop_toTopOf="@+id/google_login_btn"
        app:srcCompat="@drawable/google_symbol" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_marginStart="20dp"
        android:translationZ="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/kakao_login_btn"
        app:layout_constraintStart_toStartOf="@+id/kakao_login_btn"
        app:layout_constraintTop_toTopOf="@+id/kakao_login_btn"
        app:srcCompat="@drawable/kakao_symbol" />


</androidx.constraintlayout.widget.ConstraintLayout>